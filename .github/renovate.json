{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "docker:pinDigests"
  ],
  "packageRules": [
    {
      "matchFileNames": [
        ".github/workflows/**"
      ],
      "pinDigests": false
    },
    {
      "matchManagers": [
        "custom.regex"
      ],
      "matchFileNames": [
        "^.*-example\\.yml$"
      ],
      "pinDigests": false
    }
  ]
}
